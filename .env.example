# Variables de conexión a PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=n8n

# Configuración de n8n
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=pgvector
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=${POSTGRES_DB}
DB_POSTGRESDB_USER=${POSTGRES_USER}
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}

# Autenticación de n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=
N8N_BASIC_AUTH_PASSWORD=

# Enforce permissions on settings file
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_RUNNERS_ENABLED=true
N8N_ENCRYPTION_KEY=
N8N_USER_MANAGEMENT_JWT_SECRET=

# URL pública para los webhooks de n8n
WEBHOOK_URL=https://mi-dominio.com

# Configuración del host y puerto de n8n
N8N_HOST=mi-dominio.com
N8N_PROTOCOL=https
N8N_PORT=5678
N8N_SECURE_COOKIE=true

# Otras configuraciones de n8n
N8N_DISABLE_PRODUCTION_MAIN_PROCESS=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=true
